{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "index_gemini.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/gemini/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/",
      "dest": "index_gemini.html"
    },
    {
      "src": "/(.*)",
      "dest": "index_gemini.html"
    }
  ]
}
